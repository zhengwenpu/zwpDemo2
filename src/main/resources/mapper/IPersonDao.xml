<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zwp.dao.IPersonDao">
    <select id="findAll" resultType="com.zwp.entity.Person">
        select id,userName,addr  from person
    </select>
    <select id="findByid" resultType="com.zwp.entity.Person">
        select * from person where id = #{id}
    </select>
</mapper>
